<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".PaymentSummaryActivity">

	<TextView
			android:text="Payment Succesfully Done"
			android:layout_width="0dp"
			android:textSize="20sp"
			android:textAlignment="center"
			android:textColor="@color/colorBlack"
			android:layout_height="wrap_content"
			android:id="@+id/textView16" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
			app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toBottomOf="@+id/imageView4"
			android:layout_marginTop="16dp"/>
	<ImageView
			android:layout_width="100dp"
			android:layout_height="100dp" app:srcCompat="@drawable/ic_payment_done"
			android:id="@+id/imageView4" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
	<TextView
			android:text="Order ID :"
			android:textSize="18sp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView17" app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="16dp" app:layout_constraintTop_toBottomOf="@+id/textView16"
			android:layout_marginTop="50dp"/>
	<TextView
			android:text="TextView"
			android:textSize="18sp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView19" app:layout_constraintStart_toEndOf="@+id/textView17"
			android:layout_marginStart="8dp"
			app:layout_constraintTop_toTopOf="@+id/textView17" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="16dp"/>
	<TextView
			android:text="Amount :"
			android:textSize="18sp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView20" app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="16dp" app:layout_constraintTop_toBottomOf="@+id/textView17"
			android:layout_marginTop="16dp"/>
	<TextView
			android:text="TextView"
			android:textSize="18sp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView21" app:layout_constraintStart_toEndOf="@+id/textView20"
			android:layout_marginStart="8dp" app:layout_constraintTop_toTopOf="@+id/textView20"
			app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"/>
	<TextView
			android:text="Thank you for shoppping on TravelPA, Wish you a happy journey"
			android:layout_width="0dp"
			android:textSize="18sp"
			android:textAlignment="center"
			android:textColor="@color/colorBlack"
			android:textStyle="bold"
			android:layout_height="wrap_content"
			android:id="@+id/textView22" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
			app:layout_constraintTop_toBottomOf="@+id/textView20" android:layout_marginBottom="8dp"
			app:layout_constraintBottom_toBottomOf="parent"/>
	<Button
			android:text="Home"
			android:background="@color/colorWhite"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:id="@+id/button4" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
			app:layout_constraintEnd_toStartOf="@+id/button5" android:layout_marginEnd="8dp"
			android:layout_marginBottom="16dp" app:layout_constraintBottom_toBottomOf="parent"/>
	<Button
			android:text="My Trips"
			android:background="@drawable/login_bt_bg"
			android:layout_width="0dp"
			android:textColor="@color/colorWhite"
			android:layout_height="wrap_content"
			android:id="@+id/button5" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
			app:layout_constraintStart_toEndOf="@+id/button4" android:layout_marginStart="8dp"
			android:layout_marginBottom="16dp" app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>